<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Portoa.Web.Rest.MvcTest" namespace="Portoa.Web.Rest.MvcTest.Controllers">

	<class name="User" table="rest_user">
		<id column="user_id" name="Id" type="int">
			<generator class="identity" />
		</id>

		<property name="Username" column="username" not-null="false" type="string" length="50"/>
		<property name="Email" column="email" not-null="false" type="string" length="50"/>
		<property name="Created" column="created" not-null="true" />
	</class>

</hibernate-mapping>